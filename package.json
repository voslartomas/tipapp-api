{
  "name": "tipapp-api",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "start": "npm run build && npm run generateDocs && cd ./build && node ./index.js ",
    "start:production": "npm run build && cd ./build && NODE_ENV=production node ./index.js ",
    "pretest": "npm run tslint",
    "test": "NODE_ENV=test NODE_CONFIG_DIR=./src/config jest --forceExit --coverage --verbose --config=jest.config.json",
    "test-win": "SET NODE_ENV=test NODE_CONFIG_DIR=./src/config jest --forceExit --coverage --verbose --config=jest.config.json",
    "test-coveralls": "npm run test && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "test-dependecies": "depcheck --ignores=depcheck,pre-commit,@types/*,jest,source-map-support,ts-jest",
    "tslint": "tslint -c tslint.json -p tsconfig.json --fix",
    "copyStatic": "cp -r ./src/config ./build/ && cp -r ./src/definitions ./build/",
    "generateDocs": "./node_modules/.bin/swaggerGen -c swaggerConfig.json",
    "build": "rm -rf ./build && tsc && npm run tslint && npm run copyStatic",
    "dev": "./node_modules/.bin/nodemon | bunyan -o short",
    "db:create": "./node_modules/.bin/sequelize db:create",
    "fixtures": "npm run build && cd ./build && node ./importFixtures.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voslartomas/tipapp-api.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/voslartomas/tipapp-api/issues"
  },
  "homepage": "https://github.com/voslartomas/tipapp-api#readme",
  "dependencies": {
    "@types/bunyan": "^1.8.4",
    "@types/es6-promise": "^3.3.0",
    "@types/express": "^4.11.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "bunyan": "^1.8.12",
    "bunyan-middleware": "^0.8.0",
    "config": "^3.3.8",
    "cors": "^2.8.4",
    "coveralls": "^3.0.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.0",
    "moment": "^2.22.2",
    "node-cron": "^1.2.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.4.1",
    "redis": "^2.8.0",
    "request": "^2.87.0",
    "sequelize": "^4.37.4",
    "sequelize-cli": "^4.0.0",
    "sequelize-typescript": "^0.6.3",
    "sqlite3": "^4.0.0",
    "superagent": "^3.8.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "typescript-ioc": "^1.1.1",
    "typescript-rest": "^1.3.0",
    "typescript-rest-swagger": "0.0.17"
  },
  "devDependencies": {
    "@types/cors": "^2.8.3",
    "@types/jest": "^22.1.3",
    "@types/superagent": "^3.5.7",
    "@types/supertest": "^2.0.4",
    "depcheck": "^0.6.8",
    "jest": "^29.3.1",
    "nock": "^9.1.9",
    "nodemon": "^1.17.2",
    "pre-commit": "^1.2.2",
    "sinon": "^4.4.10",
    "source-map-support": "^0.5.2",
    "supertest": "^3.0.0",
    "ts-jest": "^29.0.3"
  }
}
